        {
            "episode": "ep-675",
            "act": "act1",
            "act_title": "Act One: Anything You Say Can And Will Be Used...on Television",
            "role": "host",
            "speaker": "dan taberski",
            "utterance_start": 1651.43,
            "utterance_end": 1687.33,
            "duration": 35.899999999999864,
